{
  "name": "badass-react-redux-node-boilerplate",
  "version": "0.1.0",
  "description": "React Redux Node Boilerplate",
  "author": "Jeremy Bernier",
  "main": "index.prod.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:prod:clean": "npm run clean && npm run build:prod && npm run start:prod",
    "start:dev": "NODE_ENV=development node index.dev.js",
    "start:prod": "NODE_ENV=production node index.prod.js",
    "start:staging": "NODE_ENV=staging node index.prod.js",
    "watch:dev": "NODE_ENV=development nodemon index.dev.js",
    "build": "npm run build:prod",
    "build:server": "./node_modules/.bin/babel src -d bin && npm run build:server:symlink",
    "build:server:symlink": "rm -rf node_modules/@app && ln -sf ../bin node_modules/@app",
    "build:prod": "webpack --config webpack/production/prod.config.js && npm run build:server",
    "build:dev": "webpack --config webpack/development/dev.config.js",
    "clean": "rm -rf build && rm -rf bin"
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.15.2",
    "bootstrap": "4.0.0-alpha.6",
    "connect-ensure-login": "0.1.1",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-alpha.1",
    "crypto": "^0.0.3",
    "express": "4.14.0",
    "history": "3.2.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.19",
    "lost": "7.1.0",
    "mobile-detect": "^1.3.5",
    "node-sass": "4.5.0",
    "optimizely-client-sdk": "^1.3.4",
    "optimizely-server-sdk": "^1.2.2",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "path-to-regexp": "1.5.3",
    "platform": "1.3.3",
    "qs": "https://registry.npmjs.org/qs/-/qs-6.2.0.tgz",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-dom": "15.3.2",
    "react-helmet": "^5.0.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "react-router-scroll": "0.4.1",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.24.0",
    "chunk-manifest-webpack-plugin": "1.0.0",
    "css-loader": "0.27.3",
    "eslint": "3.17.1",
    "eslint-loader": "1.6.3",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.9.0",
    "nodemon": "1.10.2",
    "postcss-assets": "4.1.0",
    "postcss-cssnext": "2.9.0",
    "postcss-loader": "1.3.3",
    "postcss-scss": "0.4.1",
    "precss": "1.4.0",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-logger": "2.6.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.13.2",
    "url-loader": "0.5.7",
